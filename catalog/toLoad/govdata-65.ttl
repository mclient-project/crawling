<https://gdk.gdi-de.org/inspire/srv/eng/xml_iso19139?uuid=D42A83D3-8D1A-45F9-9FD3-BEB8C51E091D>
    dcatde:contributorID <http://dcat-ap.de/def/contributors/gdiDE> ;
